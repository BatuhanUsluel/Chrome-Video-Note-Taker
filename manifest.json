{
  "manifest_version": 2,
  "name": "Video Note Taker",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.5.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "<all_urls>",
    "webNavigation"
  ],
  "web_accessible_resources": ["popup.html"]
}